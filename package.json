    "concurrently": "^3.5.1",
    "cpx": "^1.5.0",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.5.5",
    "mocha": "^5.2.0",
    "nsp": "^3.2.1",
    "nyc": "^11.9.0",
    "power-assert": "^1.5.0",
    "rimraf": "^2.6.2",
    "sinon": "^4.5.0",
    "snyk": "^1.82.1",
    "ts-node": "^4.1.0",
    "tslint": "^5.10.0",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "^2.9.1"
  },
  "engines": {
    "node": ">=8.0.0",
